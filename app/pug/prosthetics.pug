- var img = '../img/';

doctype
html
	include blocks/head.pug
		title
	body
		include blocks/header.pug

		main.page-container

			section.breadcrumbs
				.container
					ul.list
						li
							a(href="index.html") На главную
						li
							a(href="prosthetics.html") Протезирование зубов

			section.mainblock.type3
				.slider
					.slide
						.slide-left
							img.slide__img(src=img+"mainblock/prosthetics-img.jpg", alt="")
						.slide-right
							.block-title Протезирование зубов <span>с гарантией до 15 лет!</span> 
							.block-subtitle Брекет-системы на любой бюджет!
							.block-text Записаться <span>на бесплатную консультацию:</span>
