		include blocks/header.pug

		main.page-container

			.circle1
			.circle2

			section.breadcrumbs
				.container
					ul.list
						li
							a(href="index.html") На главную
						li
							a(href="prices.html") Цены

			section.mainblock.type2
				.slider
					.slide
						.slide-left
							img.slide__img(src=img+"mainblock/prices-img.jpg", alt="")
						.slide-right
							.block-title Цены на наши услуги
							.block-text У нас нет скрытых платежей - все цены фиксируются в договоре <br> и в дальнейшем не меняются. <br> Качественная работа по приемлемым ценам!
							include blocks/_entry-form.pug

			section.advantages
				.container
					.item
						.img-wrap
							img.item__img(src=img+"icons/tag.svg", alt="")
						.item__title Честные цены
						.item__text Действует прозрачный расчёт на итоговую стоимость
